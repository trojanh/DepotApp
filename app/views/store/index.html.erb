<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>

<% for product in @products %>
  <div class="entry">
    <%= image_tag(product.image_url) %>
    <h3><%= product.title %> </h3>
    <%= sanitize(product.description) %>
    <div class="price_line">
      <span class='price'><%=product.price%></span>
    </div>
  </div>
<%end%>
<p>Find me in app/views/store/index.html.erb</p>


